{
    "app": "watson_assistant",
    "version": "0.0.1",
    "title": "Watson Assistant App",
    "dependencies": ["kustomer-^1.5.0'"],
    "iconUrl": "data:image/png;base64,",
    "description": "Watson Assistant App",
    "appDetails": {
      "appDeveloper": {
        "name": "IBM",
        "website": "https://www.ibm.com/products/watson-assistant",
        "supportEmail": "support@ibm.com"
      },
      "documentationLinks": [
        {
          "title": "Getting Started",
          "url": "https://github.com/watson-developer-cloud/assistant-web-chat-service-desk-starter"
        }
      ]
    },
    "klasses": [{
        "name": "conversation",
        "metadata": {
            "standard": true,
            "properties": {
                "watsonAssistantSessionHistoryKeyStr": {
                    "displayName": "Watson Assistant Session History"
                }
            }
        }
    }],
    "kviews": [{
        "name": "watson_assistant.chatHistory",
        "resource": "conversation",
        "context": "smartbar-card",
        "meta": {
            "icon": "bot",
            "displayName": "Watson Assistant Chat History"
        },
        "template": ""
    }]
  }